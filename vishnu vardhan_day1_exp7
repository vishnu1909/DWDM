cat("vishnu vardhan_192110147_day01\n")
pencil_counts <- c(9, 25, 23, 12, 11, 6, 7, 8, 9, 10)
mean_value <- mean(pencil_counts)
median_value <- median(pencil_counts)
mode_result <- table(pencil_counts)
modes <- as.numeric(names(mode_result[mode_result == max(mode_result)]))
cat("Pencil Counts:", pencil_counts, "\n")
cat("(a) Mean:", mean_value, "\n")
cat("(b) Median:", median_value, "\n")
cat("(c) Mode(s):", modes, "\n")
