cat("vishnu vardhan_192110147_day01\n")
phones_sold <- c(4, 1, 5, 7, 10, 2, 50, 25, 90, 36)
money <- c(12, 5, 13, 19, 31, 7, 153, 72, 275, 110)
plot(phones_sold, money, main = "Scatter Plot of Mobile Phones Sold",
     xlab = "Number of Mobile Phones Sold", ylab = "Money")
text(phones_sold, money, labels = seq_along(phones_sold), pos = 3, col = "blue")
