cat("vishnu vardhan_192110147_day02")
data(mtcars)
plot(mtcars$mpg, type = "l", col = "blue", lty = 1, lwd = 2,
     xlab = "Index", ylab = "mpg and qsec", main = "Line Chart with Multiple Lines")
lines(mtcars$qsec, col = "red", lty = 2, lwd = 2)
legend("topright", legend = c("mpg", "qsec"), col = c("blue", "red"), lty = c(1, 2), lwd = 2)
